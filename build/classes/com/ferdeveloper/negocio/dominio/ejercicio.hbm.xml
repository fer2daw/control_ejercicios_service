<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.ferdeveloper.negocio.dominio.Ejercicio" table="ejercicios">
    <id column="idEjercicio" name="idEjercicio" type="integer"/>
    <property name="nombreEjercicio"/>
    <property name="descripcionEjercicio"/>
    <property name="categoriaEjercicio"/>
    <property name="fechaEjercicio"/>
    <property name="observacionesEjercicio"/>
</class>
</hibernate-mapping>